{
  "name": "drupal/recommended-project",
  "description": "Project template for Drupal 9 projects with a relocated document root",
  "type": "project",
  "license": "GPL-2.0-or-later",
  "homepage": "https://www.drupal.org/project/drupal",
  "support": {
      "docs": "https://www.drupal.org/docs/user_guide/en/index.html",
      "chat": "https://www.drupal.org/node/314178"
  },
  "repositories": [
      {
          "type": "composer",
          "url": "https://packages.drupal.org/8"
      }
  ],
  "require": {
    "abraham/twitteroauth": "^3.1",
    "composer/installers": "^1.9",
    "cviebrock/discourse-php": "^0.9.3",
    "cweagans/composer-patches": "^1.7",
    "drupal/acquia_connector": "^3.0",
    "drupal/address": "^1.9",
    "drupal/admin_toolbar": "^3.0",
    "drupal/adminimal_admin_toolbar": "^1.11",
    "drupal/adminimal_theme": "^1.6",
    "drupal/better_exposed_filters": "^5.0@beta",
    "drupal/block_class": "^1.3",
    "drupal/button_link": "^1.2",
    "drupal/config_ignore": "^2.3",
    "drupal/contact_block": "^1.5",
    "drupal/contact_storage": "^1.1",
    "drupal/core-composer-scaffold": "^9.2",
    "drupal/core-project-message": "^9.2",
    "drupal/core-recommended": "^9.2",
    "drupal/ctools": "^3.7",
    "drupal/current_page_crumb": "^1.3",
    "drupal/easy_breadcrumb": "^2.0",
    "drupal/editor_advanced_link": "^2.0",
    "drupal/editor_file": "^1.5",
    "drupal/embed": "^1.4",
    "drupal/entity_browser": "^2.6",
    "drupal/entity_embed": "^1.2",
    "drupal/entity_print": "^2.4",
    "drupal/entityqueue": "^1.2",
    "drupal/externalauth": "^1.4",
    "drupal/fast_404": "2.x-dev",
    "drupal/field_formatter": "^3.0@alpha",
    "drupal/field_group": "^3.2",
    "drupal/file_browser": "^1.3",
    "drupal/flag": "^4.0@beta",
    "drupal/forward": "^4.0",
    "drupal/geocoder": "^3.20",
    "drupal/geolocation": "^3.7",
    "drupal/google_analytics": "^3.1",
    "drupal/honeypot": "^2.0",
    "drupal/image_widget_crop": "^2.3",
    "drupal/inline_entity_form": "^1.0@RC",
    "drupal/insert": "^1.0",
    "drupal/jquery_colorpicker": "^2.0@RC",
    "drupal/key": "^1.14",
    "drupal/maillog": "^1.0@beta",
    "drupal/masquerade": "^2.0@beta",
    "drupal/media_entity_browser": "^2.0@alpha",
    "drupal/media_entity_image": "^1.3",
    "drupal/metatag": "^1.16",
    "drupal/module_filter": "^3.2",
    "drupal/paragraphs": "^1.12",
    "drupal/pathauto": "^1.8",
    "drupal/persistent_login": "^1.3",
    "drupal/recaptcha": "^3.0",
    "drupal/recreate_block_content": "^2.1",
    "drupal/redirect": "^1.6",
    "drupal/scheduler": "^1.4",
    "drupal/seckit": "^2.0",
    "drupal/services": "^4.0@beta",
    "drupal/shield": "^1.4",
    "drupal/smart_trim": "^1.3",
    "drupal/svg_image": "^1.15",
    "drupal/token": "^1.9",
    "drupal/twig_tweak": "^2.9",
    "drupal/upgrade_status": "^3.11",
    "drupal/username_enumeration_prevention": "^1.2",
    "drupal/vendor_stream_wrapper": "^1.6",
    "drupal/views_infinite_scroll": "^1.9",
    "drupal/webform": "^6.0",
    "drupal/xmlsitemap": "^1.1",
    "drush/drush": "^10.6"
  },
  "require-dev": {
      "drupal/devel": "^4.1",
      "drupal/stage_file_proxy": "^1.1",
      "drupal/twig_vardumper": "^3.0",
      "drupal/twig_xdebug": "^1.2"
  },
  "conflict": {
      "drupal/drupal": "*"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
      "sort-packages": true
  },
  "extra": {
      "drupal-scaffold": {
          "locations": {
              "web-root": "docroot/"
          }
      },
      "installer-paths": {
          "docroot/core": [
              "type:drupal-core"
          ],
          "docroot/libraries/{$name}": [
              "type:drupal-library"
          ],
          "docroot/modules/contrib/{$name}": [
              "type:drupal-module"
          ],
          "docroot/profiles/contrib/{$name}": [
              "type:drupal-profile"
          ],
          "docroot/themes/contrib/{$name}": [
              "type:drupal-theme"
          ],
          "drush/Commands/contrib/{$name}": [
              "type:drupal-drush"
          ],
          "docroot/modules/custom/{$name}": [
              "type:drupal-custom-module"
          ],
          "docroot/profiles/custom/{$name}": [
              "type:drupal-custom-profile"
          ],
          "docroot/themes/custom/{$name}": [
              "type:drupal-custom-theme"
          ]
      },
      "drupal-core-project-message": {
          "include-keys": [
              "homepage",
              "support"
          ],
          "post-create-project-cmd-message": [
              "<bg=blue;fg=white>                                                         </>",
              "<bg=blue;fg=white>  Congratulations, youâ€™ve installed the Drupal codebase  </>",
              "<bg=blue;fg=white>  from the drupal/recommended-project template!          </>",
              "<bg=blue;fg=white>                                                         </>",
              "",
              "<bg=yellow;fg=black>Next steps</>:",
              "  * Install the site: https://www.drupal.org/docs/8/install",
              "  * Read the user guide: https://www.drupal.org/docs/user_guide/en/index.html",
              "  * Get support: https://www.drupal.org/support",
              "  * Get involved with the Drupal community:",
              "      https://www.drupal.org/getting-involved",
              "  * Remove the plugin that prints this message:",
              "      composer remove drupal/core-project-message"
          ]
      },
      "merge-plugin": {
        "include": [
            "modules/custom/msca_tweet_block/composer.json"
        ],
        "recurse": false,
        "replace": false,
        "merge-extra": false
    }
  }
}
