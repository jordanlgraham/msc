{
    "name": "handsomedogstudio/msc",
    "description": "Drupal 8 website for Massachusetts Senior Care Association.",
    "type": "project",
    "license": "GPL-2.0+",
    "repositories": [
      {
        "type": "composer",
        "url": "https://packages.drupal.org/8"
      }
    ],
    "require": {
        "abraham/twitteroauth": "^0.7.4",
        "composer/installers": "^1.0.21",
        "cviebrock/discourse-php": "0.9.3",
        "drupal/acquia_connector": "~1.8",
        "drupal/address": "~1.0",
        "drupal/admin_toolbar": "^2.2",
        "drupal/adminimal_admin_toolbar": "^1.4",
        "drupal/adminimal_theme": "^1.3",
        "drupal/better_exposed_filters": "^5.0@beta",
        "drupal/block_class": "^1.0@alpha",
        "drupal/button_link": "^1.0",
        "drupal/config_ignore": "2.1",
        "drupal/config_installer": "1.4",
        "drupal/config_split": "1.0-beta6",
        "drupal/contact_block": "^1.4",
        "drupal/contact_storage": "^1.0@beta",
        "drupal/core-composer-scaffold": "^8.8",
        "drupal/core-project-message": "^8.8",
        "drupal/core-recommended": "^8.8",
        "drupal/ctools": "3.0.0",
        "drupal/current_page_crumb": "1.1",
        "drupal/devel_entity_updates": "^3.0",
        "drupal/easy_breadcrumb": "^1.13",
        "drupal/editor_advanced_link": "1.4",
        "drupal/editor_file": "1.2",
        "drupal/embed": "1.0",
        "drupal/entity_browser": "2.*",
        "drupal/entity_embed": "^1.0",
        "drupal/entity_print": "2.x",
        "drupal/entityqueue": "^1.0@alpha",
        "drupal/external_entities": "^1.0@alpha",
        "drupal/externalauth": "^1.0",
        "drupal/fast_404": "^2.0@alpha",
        "drupal/field_formatter": "^1.0",
        "drupal/field_group": "3.x-dev",
        "drupal/flag": "4.0.0-alpha2",
        "drupal/forward": "2.0-rc2",
        "drupal/geocoder": "2.*",
        "drupal/geofield": "^1.0@alpha",
        "drupal/geolocation": "3.0",
        "drupal/google_analytics": "^2.1",
        "drupal/honeypot": "^1.29",
        "drupal/image_widget_crop": "2.0",
        "drupal/inline_entity_form": "1.0-beta1",
        "drupal/insert": "^2.0@beta",
        "drupal/jquery_colorpicker": "^2.0",
        "drupal/maillog": "^1.0@beta",
        "drupal/masquerade": "2.0-beta1",
        "drupal/media_entity": "^2.0",
        "drupal/media_entity_browser": "^2.0",
        "drupal/media_entity_image": "^1.2",
        "drupal/metatag": "^1.12",
        "drupal/module_filter": "^3.1",
        "drupal/paragraphs": "^1.1",
        "drupal/pathauto": "^1.8",
        "drupal/persistent_login": "^1.1",
        "drupal/recaptcha": "^2.3",
        "drupal/redirect": "^1.0@alpha",
        "drupal/rules": "^3.0@alpha",
        "drupal/scheduler": "1.0",
        "drupal/seckit": "^1.0@alpha",
        "drupal/services": "^4.0@beta",
        "drupal/shield": "^1.0",
        "drupal/smart_trim": "^1.0",
        "drupal/svg_image": "^1.2",
        "drupal/token": "1.0.0",
        "drupal/twig_tweak": "^1.8",
        "drupal/twig_xdebug": "^1.0",
        "drupal/username_enumeration_prevention": "^1.0@beta",
        "drupal/vendor_stream_wrapper": "^1.6",
        "drupal/views_infinite_scroll": "^1.5",
        "drupal/webform": "^5.0@beta",
        "drupal/xmlsitemap": "^1.0@alpha",
        "drush/drush": "^10.0",
        "mikehaertl/phpwkhtmltopdf": "~2.1",
        "php-http/guzzle6-adapter": "^1.1",
        "symfony/dom-crawler": "3.*",
        "tecnickcom/tcpdf": "~6",
        "willdurand/geocoder": "3.*"
    },
    "require-dev": {
        "drupal/console": "^1.9",
        "drupal/devel": "^2.1",
        "drupal/stage_file_proxy": "^1.1"
    },
    "conflict": {
      "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
      "sort-packages": true
    },
    "extra": {
      "drupal-scaffold": {
        "locations": {
          "web-root": "docroot/"
        }
      },
      "installer-paths": {
        "docroot/core": [
          "type:drupal-core"
        ],
        "docroot/libraries/{$name}": [
          "type:drupal-library"
        ],
        "docroot/modules/contrib/{$name}": [
          "type:drupal-module"
        ],
        "docroot/profiles/contrib/{$name}": [
          "type:drupal-profile"
        ],
        "docroot/themes/contrib/{$name}": [
          "type:drupal-theme"
        ],
        "drush/Commands/contrib/{$name}": [
          "type:drupal-drush"
        ],
        "docroot/modules/custom/{$name}": [
          "type:drupal-custom-module"
        ],
        "docroot/themes/custom/{$name}": [
          "type:drupal-custom-theme"
        ]
      }
    }
  }
