{
  "name": "drupal/drupal",
  "description": "Drupal is an open source content management platform powering millions of websites and applications.",
  "type": "project",
  "license": "GPL-2.0+",
  "repositories": [
    {
      "type": "composer",
      "url": "https://packages.drupal.org/8"
    }
  ],
  "require": {
    "composer/installers": "^1.0.21",
    "drupal/acquia_connector": "~1.8",
    "drupal/address": "~1.0",
    "drupal/admin_toolbar": "^1.19",
    "drupal/better_exposed_filters": "^3.0@alpha",
    "drupal/block_class": "^1.0@alpha",
    "drupal/contact_storage": "^1.0@beta",
    "drupal/core": "~8.5",
    "drupal/ctools": "3.0.0",
    "drupal/devel": "^1.0@RC",
    "drupal/entityqueue": "^1.0@alpha",
    "drupal/external_entities": "^1.0@alpha",
    "drupal/externalauth": "^1.0",
    "drupal/fast_404": "^1.0@alpha",
    "drupal/field_formatter": "^1.0",
    "drupal/geocoder": "2.x-dev",
    "drupal/geofield": "^1.0@alpha",
    "drupal/geolocation": "^1.10",
    "drupal/google_analytics": "^2.1",
    "drupal/masquerade": "2.0-beta1",
    "drupal/metatag": "1.0",
    "drupal/pathauto": "1.0.0",
    "drupal/redirect": "^1.0@alpha",
    "drupal/rules": "^3.0@alpha",
    "drupal/seckit": "^1.0@alpha",
    "drupal/services": "^4.0@beta",
    "drupal/shield": "^1.0",
    "drupal/token": "1.0.0",
    "drupal/username_enumeration_prevention": "^1.0@beta",
    "drupal/xmlsitemap": "^1.0@alpha",
    "php-http/guzzle6-adapter": "^1.1",
    "wikimedia/composer-merge-plugin": "~1.3",
    "willdurand/geocoder": "^3.3",
    "drupal/entity_print": "2.x",
    "drupal/forward": "2.0-rc2",
    "drupal/flag": "4.0.0-alpha2",
    "drupal/inline_entity_form": "1.0-beta1",
    "mikehaertl/phpwkhtmltopdf": "~2.1",
    "tecnickcom/tcpdf": "~6",
    "drupal/adminimal_theme": "^1.3",
    "drupal/adminimal_admin_toolbar": "^1.4",
    "drupal/insert": "1.0-beta1",
    "drupal/image_widget_crop": "2.0",
    "drupal/twig_tweak": "^1.8",
    "drupal/editor_advanced_link": "1.4",
    "drupal/editor_file": "1.2",
    "drupal/media_entity": "1.6",
    "drupal/media_entity_browser": "1.0-beta3",
    "drupal/media_entity_image": "1.2",
    "drupal/embed": "1.0",
    "drupal/entity_embed": "1.x-dev",
    "drupal/paragraphs": "^1.1",
    "drupal/config_installer": "1.4",
    "drupal/config_split": "1.0-beta6",
    "drupal/current_page_crumb": "1.1",
    "drupal/easy_breadcrumb": "1.6",
    "drupal/button_link": "^1.0",
    "drupal/field_group": "3.x-dev",
    "cviebrock/discourse-php": "0.9.3",
    "drupal/jquery_colorpicker": "^1.3",
    "drupal/svg_image": "^1.2",
    "drupal/contact_block": "^1.4",
    "drupal/views_infinite_scroll": "^1.5",
    "abraham/twitteroauth": "^0.7.4",
    "drupal/config_ignore": "2.1",
    "drupal/scheduler": "1.0",
    "drupal/webform": "^5.0@beta",
    "drupal/smart_trim": "^1.0",
    "drush/drush": "^9.3",
    "drupal/honeypot": "^1.29"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.8"

  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "preferred-install": "dist",
    "autoloader-suffix": "Drupal8"
  },
  "extra": {
    "_readme": [
      "By default Drupal loads the autoloader from ./vendor/autoload.php.",
      "To change the autoloader you can edit ./autoload.php."
    ],
    "merge-plugin": {
      "include": [
        "core/composer.json"
      ],
      "recurse": false,
      "replace": false,
      "merge-extra": false
    },
    "installer-paths": {
      "libraries/{$name}": ["type:drupal-library"],
      "modules/contrib/{$name}": ["type:drupal-module"]
    },
    "patches": {
      "drupal/button_link": {
        "Adding missing outline styles, adding unset array key": "patches/contrib/button_link/button_link_formatter.patch"
      },
      "drupal/address": {
        "The locality.name data is missed in PlainFormater": "patches/contrib/address/locality-name-data-missed-plainformater-2881391-1.patch"
      }
    }
  },
  "autoload": {
    "psr-4": {
      "Drupal\\Core\\Composer\\": "core/lib/Drupal/Core/Composer"
    }
  },
  "scripts": {
    "pre-autoload-dump": "Drupal\\Core\\Composer\\Composer::preAutoloadDump",
    "post-autoload-dump": "Drupal\\Core\\Composer\\Composer::ensureHtaccess",
    "post-package-install": "Drupal\\Core\\Composer\\Composer::vendorTestCodeCleanup",
    "post-package-update": "Drupal\\Core\\Composer\\Composer::vendorTestCodeCleanup"
  }
}
